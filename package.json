{
  "name": "vue-design-web",
  "version": "1.0.0",
  "description": "A Vue.js project",
  "author": "n.see <gooarter@gmail.com>",
  "private": true,
  "scripts": {
    "serve": "npx vue-cli-service serve",
    "build": "npx vue-cli-service build",
    "lint": "npx vue-cli-service lint"
  },
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "core-js": "^3.6.4",
    "element-ui": "^2.13.0",
    "js-cookie": "^2.2.1",
    "list-to-tree": "^2.2.3",
    "lodash": "^4.17.15",
    "vue": "^2.6.11",
    "vue-cropperjs": "^4.1.0",
    "vue-design-core": "^0.1.8",
    "vue2-ace-editor": "0.0.15",
    "vuex": "^3.1.3",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-service": "^4.3.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.1.2",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-useless-constructor": "off",
      "semi": ["error", "always"],
      "indent": ["error", 4],
      "space-before-function-paren": ["error", "never"],
      "generator-star-spacing": "off",
      "no-debugger": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
